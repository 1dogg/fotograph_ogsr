hit_big_particles			= ;получение хита - эффект полного удара аномалии
idle_small_particles		=
idle_big_particles			= 
entrance_small_particles	= 
entrance_big_particles		= 

tele_particles_small		= 
tele_particles_big			= 	


idle_sound			= anomaly\no_sound;  							;постоянный звук
blowout_sound		= anomaly\no_sound;во время выброса(удара) в центре аномалии	
hit_sound			= anomaly\no_sound		;на персонаже, когда тот получает хит
body_tearing_sound	= anomaly\no_sound;			;звук разрывания трупа
entrance_sound		= anomaly\no_sound;						;при попадании объекта в аномалию

hit_type		    = strike
hit_impulse_scale	= 0.0;0.1;0.1			;соотношение физического импульса и силы хита
effective_radius	= 0,05 ;размер радиуса в процентах от оригинального, где действует зона

disable_time		= 5000	;время игнорирования неживого объекта в зоне (-1 если не нужно)
disable_time_small	= 5000		;время игнорирования маленького неживого объекта в зоне (-1 если не нужно)
disable_idle_time	= 3	;время отключения idle партиклов

ignore_nonalive		= true
ignore_small		= true
ignore_artefacts	= true

blowout_light		= off
light_color		    = 0,0,0
light_range		    = 0.0
light_time 		    = 0.8;0.35
light_height		= -5			;подъем источника света на высоту

idle_light			= off
idle_light_range			= 0.0
idle_light_range_delta 		= 0.0
idle_light_anim				= 
idle_light_height			= -5			;подъем источника света на высоту

awaking_time		= 800;1000
blowout_time		= 1000	; время выброса энергии аномалии
accamulate_time		= 500

blowout_light_time		= 800 ; свет при выбросе энергии аномалии
blowout_explosion_time	= 800 ; время взрыва энергии аномалии

;параметры телекинеза для зоны
throw_in_impulse			= 4000	;сила втягивания для единичной массы на расстоянии один метр
throw_out_impulse				= 1500			;сила выбрасывания взрывом
throw_in_impulse_alive			= 600	;сила импульса втягивания для живых
throw_in_atten					= 2.2	;3;коэфф. затягивания (чем меньше, тем плавнее затягивает)
throw_in_atten				= 		8;8			;коэфф. затягивания (чем меньше, тем плавнее затягивает)
blowout_radius_percent		= 		0.8		;радиус (в процентах от всего радиуса) непосредственно выброса
actor_blowout_radius_percent=		0.05

tele_height				= 0.05;	3.5	;высота подъема телекинеза 
time_to_tele			= 5000		;время деражания объекта в воздухе
tele_pause				= 5000		;пауза перед тем как снова поднять упавший объект	

ignore_light		= true

visible_by_detector = off


;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; ветер
blowout_wind		= on

blowout_wind_time_start     = 0
blowout_wind_time_peak      = 800
blowout_wind_time_end		= 1001; 2499
blowout_wind_power			= 0.5			;сила поднимаего ветра (от 0 до 1), в момент blowout_wind_time_peak 



;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;рождение артефактов во время срабатывания
