[logic]
active = sr_idle@one

[sr_idle@one]
on_actor_inside = sr_idle@two %=run_postprocess(blink) =tunnel.set_finish =bind_stalker.hide_weapon% 

[sr_idle@two]
on_info = {+tunnel_transfer} sr_idle@three  

[sr_idle@three]
on_timer = 5000 | nil %=tunnel.del_tunnel -tunnel_transfer%
on_actor_inside = sr_idle@four

[sr_idle@four]
on_info = {+tunnel_transfer} sr_idle@three %=run_postprocess(blink) =tunnel.set_dist%