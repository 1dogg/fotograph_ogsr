[logic]
active = {-info_valagro_03 -info_valagro_04} sr_idle@one, {+info_valagro_03 -info_valagro_04} sr_idle@tri, {+info_valagro_03 +info_valagro_04} sr_idle@five

[sr_idle@one]
on_actor_inside = {+amb_agro} sr_idle@two %=play_snd(ambient\new\dark_one12) +info_valagro_03%

[sr_idle@two]
on_timer = 90000 | sr_idle@tri

[sr_idle@tri]
on_actor_inside = {+info_valagro_03} sr_idle@four %=play_snd(ambient\new\dark_space_2) +info_valagro_04%

[sr_idle@four]
on_timer = 60000 | sr_idle@five

[sr_idle@five]
on_actor_inside = {+info_valagro_04} sr_idle@six %=play_snd(ambient\new\dark_space_3) -info_valagro_03 -info_valagro_04%

[sr_idle@six]
on_timer = 90000 | sr_idle@one 