[logic]
active = {-info_valpred_15 -info_valpred_16} sr_idle@one, {+info_valpred_15 -info_valpred_16} sr_idle@tri, {+info_valpred_15 +info_valpred_16} sr_idle@five

[sr_idle@one]
on_actor_inside = {+amb_pred} sr_idle@two %=play_snd(ambient\new\d_squeak) +info_valpred_15%

[sr_idle@two]
on_timer = 120000 | sr_idle@tri

[sr_idle@tri]
on_actor_inside = {+info_valpred_15} sr_idle@four %=play_snd(ambient\new\str_squeak_5) +info_valpred_16%

[sr_idle@four]
on_timer = 240000 | sr_idle@five

[sr_idle@five]
on_actor_inside = {+info_valpred_16} sr_idle@six %=play_snd(ambient\new\lib_debris_3) -info_valpred_15 -info_valpred_16%

[sr_idle@six]
on_timer = 2400000 | sr_idle@one 