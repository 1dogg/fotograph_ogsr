[logic]
active = {-info_valprom_35 -info_valprom_36} sr_idle@one, {+info_valprom_35 -info_valprom_36} sr_idle@tri, {+info_valprom_35 +info_valprom_36} sr_idle@five

[sr_idle@one]
on_actor_inside = {+amb_prom} sr_idle@two %=play_snd(ambient\new\ind_noise_10) +info_valprom_35%

[sr_idle@two]
on_timer = 60000 | sr_idle@tri

[sr_idle@tri]
on_actor_inside = {+info_valprom_35} sr_idle@four %=play_snd(ambient\new\ind_noise_8) +info_valprom_36%

[sr_idle@four]
on_timer = 90000 | sr_idle@five

[sr_idle@five]
on_actor_inside = {+info_valprom_36} sr_idle@six %=play_snd(ambient\new\metal_stress2) -info_valprom_35 -info_valprom_36%

[sr_idle@six]
on_timer = 120000 | sr_idle@one