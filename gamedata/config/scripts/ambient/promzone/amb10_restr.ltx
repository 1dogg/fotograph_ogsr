[sr_idle@two]
on_timer = 60000 | sr_idle@tri

[sr_idle@tri]
on_actor_inside = {+info_valprom_13} sr_idle@four %=play_snd(ambient\new\d_squeak_3) +info_valprom_14%

[sr_idle@four]
on_timer = 90000 | sr_idle@five

[sr_idle@five]
on_actor_inside = {+info_valprom_14} sr_idle@six %=play_snd(ambient\new\ind_noise_10) -info_valprom_13 -info_valprom_14%

[sr_idle@six]
