[logic]
active = {-info_vallabx_01 -info_vallabx_02} sr_idle@one, {+info_vallabx_01 -info_vallabx_02} sr_idle@tri, {+info_vallabx_01 +info_vallabx_02} sr_idle@five

[sr_idle@one]
on_actor_inside = {+amb_labx} sr_idle@two %=play_snd(ambient\new\tun_noise_3) +info_vallabx_01%

[sr_idle@two]
on_timer = 90000 | sr_idle@tri

[sr_idle@tri]
on_actor_inside = {+info_vallabx_01} sr_idle@four %=play_snd(ambient\new\tun_bkg_1b) +info_vallabx_02%

[sr_idle@four]
on_timer = 120000 | sr_idle@five

[sr_idle@five]
on_actor_inside = {+info_vallabx_02} sr_idle@six %=play_snd(ambient\new\tun_hit_7) -info_vallabx_01 -info_vallabx_02%

[sr_idle@six]
on_timer = 240000 | sr_idle@one 
