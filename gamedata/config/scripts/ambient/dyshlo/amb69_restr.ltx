[logic]
active = {-info_valdysh_11 -info_valdysh_12} sr_idle@one, {+info_valdysh_11 -info_valdysh_12} sr_idle@tri, {+info_valdysh_11 +info_valdysh_12} sr_idle@five

[sr_idle@one]
on_actor_inside = {+amb_dysh} sr_idle@two %=play_snd(ambient\new\baran_3) +info_valdysh_11%

[sr_idle@two]
on_timer = 90000 | sr_idle@tri

[sr_idle@tri]
on_actor_inside = {+info_valdysh_11} sr_idle@four %=play_snd(ambient\new\baran) +info_valdysh_12%

[sr_idle@four]
on_timer = 240000 | sr_idle@five

[sr_idle@five]
on_actor_inside = {+info_valdysh_12} sr_idle@six %=play_snd(ambient\new\baran_4) -info_valdysh_11 -info_valdysh_12%

[sr_idle@six]
on_timer = 240000 | sr_idle@one 