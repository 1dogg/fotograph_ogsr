[logic]
active = {-info_valprip_15 -info_valprip_16} sr_idle@one, {+info_valprip_15 -info_valprip_16} sr_idle@tri, {+info_valprip_15 +info_valprip_16} sr_idle@five

[sr_idle@one]
on_actor_inside = {+amb_prip} sr_idle@two %=play_snd(ambient\new\koleso) +info_valprip_15%

[sr_idle@two]
on_timer = 90000 | sr_idle@tri

[sr_idle@tri]
on_actor_inside = {+info_valprip_15} sr_idle@four %=play_snd(ambient\new\elevator_run) +info_valprip_16%

[sr_idle@four]
on_timer = 240000 | sr_idle@five

[sr_idle@five]
on_actor_inside = {+info_valprip_16} sr_idle@six %=play_snd(ambient\random_new\ind_noise_11) -info_valprip_15 -info_valprip_16%

[sr_idle@six]
on_timer = 360000 | sr_idle@one 